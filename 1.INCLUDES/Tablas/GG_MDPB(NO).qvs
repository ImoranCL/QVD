   text( CLC_MATER)  	as CLC_MATER,
   text(CLC_PLANT) 		as CLC_PLANT,
   text(CLC_VERSB) 		as CLC_VERSB,
    CLC_WBSP,
    QUANTITY,
    UniqueKey
FROM [lib://QlikRepository/1.DATOS_NIVEL_1/RAW_DATA/1.ORIGEN_1/1.QVD/0.QVD Sin textos/INI_GG_MDPB.qvd]
