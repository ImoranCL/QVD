    DATAPAKID,
    FRE_PLNPUR,
    "RECORD",
    RECORDMODE,
    REQTSN,
    SOURSYSTEM,
    UNIT,
    BDZEI,
    BEDAE,
    CUOBJ,
    ENTLI,
    KDAUF,
    KDPOS,
    KNTTP,
    KZBWS,
    KZVBR,
